{
  "name": "default-layout",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch",
    "build": "npm run clean && siteup",
    "clean": "rm -rf public && mkdir -p public",
    "watch": "npm run clean && run-p watch:*",
    "watch:serve": "browser-sync start --server 'public' --files 'public'",
    "watch:siteup": "npm run build -- --watch"
  },
  "dependencies": {
    "@siteup/cli": "../../."
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "npm-run-all2": "^6.0.0"
  },
