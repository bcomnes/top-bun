{
  "extends": "@voxpelli/tsconfig/node20.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "include": [
    "lib/**/*",
    "test-cases/**/*",
    "index.js",
    "bin.js"
  ],
  "exclude": [
    "node_modules",
    "test-cases/build-errors",
    "test-cases/page-build-errors",
    "test-cases/**/public"
  ]
}
